{
  "name": "LB-t Docker",
  "dockerComposeFile": [
    "../docker/docker-compose.yml" // Alternative: "../docker/docker-compose-gui.yml"
  ],
  "service": "lbt_docker",
  "workspaceFolder": "/code/lbt",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools"
      ]
    }
  }
}
